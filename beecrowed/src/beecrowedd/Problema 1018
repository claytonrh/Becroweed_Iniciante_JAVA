public class DoWhile {

	public static void main(String[] args) {
		
	
	int nota1 = 100;      
	int nota2 = 50;
	int nota3 = 20;
	int nota4 = 10;
	int nota5 = 5;
	int nota6 = 2;
	int soma1;
	int soma2;
	int soma3;
	int soma4;
	int soma5;
	int soma6;	
	
    int resultado = 576;
    int soma = 0;
						
		do { 		 
		 soma = soma + nota1;			
		}while(soma < resultado);	
		
		soma1 = soma - nota1;
		int qtdNota1 = soma1 / nota1;
		System.out.printf("soma das notas de 100 = " + soma1 + "\n");
		System.out.println(qtdNota1);
		
		do { 
		 soma1 = soma1 + nota2;
			
		}while(soma1 < resultado);
		
		soma2 = soma1 - nota2;
		int qtdNota2 = (soma1 - soma2) / nota2;
		System.out.printf("soma das notas de 50 = " + soma2 + "\n");
		System.out.println(qtdNota2);
		
		do { 
		 soma2 = soma2 + nota3;
			
		}while(soma2 < resultado);
		
		soma3 = soma2 - nota3;
		int qtdNota3 = (soma2 - soma3) / nota3;
		System.out.printf("soma das notas de 20 = " + soma3 + "\n");
		System.out.println(qtdNota3);
		
		do { 		
		soma3 = soma3 + nota4; 		
			
		}while(soma3 < resultado);									
				
		if((soma3 + nota4) > resultado) {
			soma4 = 0;
		}else{
			soma4 = soma3 - nota4;
			int qtdNota4 = (soma3 - soma4) / nota4;
			System.out.println(qtdNota4);
		}
		
		System.out.printf("soma das notas de 50 = " + soma4 + "\n");
		
	}
}
