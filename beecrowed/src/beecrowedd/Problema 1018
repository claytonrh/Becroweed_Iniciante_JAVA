import java.util.Scanner;
import java. util. Arrays;

public class main {

public static void main (String[] args)
{

int [] notas = {100, 50, 20, 10, 5, 2, 1};
int valor = 576;
int resultado;

for(int i : notas) {
	resultado = valor / i;	
	   System.out.printf("\n" + resultado + " nota(s) de R$ ");
	   
}		 
}
}

RETORNO:
5 nota(s) de R$ 
11 nota(s) de R$ 
28 nota(s) de R$ 
57 nota(s) de R$ 
115 nota(s) de R$ 
288 nota(s) de R$ 
576 nota(s) de R$ 

