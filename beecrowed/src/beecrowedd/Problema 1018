import java.util.Arrays;
import java.util.List;

public class Main {
	
public static void main(String[] args) {		       
	    
	List<Integer> notas = Arrays.asList(2, 3, 4);
	int valor = 10;
	int resultado;
	       

	    
	    for(int a = notas.get(0); a < valor ; a++) { 
	        for(int b = notas.get(1); b < valor; b++) { 
	            for(int c = notas.get(2); c < valor; c++) {                 

	    int soma = a+b+c;
	    if(soma == valor ) 
	    System.out.printf("%d + %d + %d = %d \n",a,b,c, soma);           
	    }
	}
}
}
}

